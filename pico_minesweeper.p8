--logic

function clear_game(diff)
 menuitem(1, "restart "..((diff==0 and "hard")or "easy"), function() clear_game(1-diff) end)
 board_t={}
 status_t={}
 for y=1,9 do
  board_t[y]={}
  status_t[y]={}
  for x=1,9 do
   board_t[y][x]=0
   status_t[y][x]=0
  end
 end
 g_diff=diff
 g_time=0
 g_mine_count=(diff==0 and 10)or 18
 g_status=0
 flag_count=0
 g_time=0
 last_l_s=0
 last_r_s=0
 last_l_time=0
 last_r_time=0
 duel_down=false
 m_btn_s=0
 key_restart=0
 --if mouse diabled
 if mx==my and omx==omy and mx==omy and mx==0 then
  key_moveto(1,1)
 end
end

function start_game(bx,by)
 c=g_mine_count
 while c>0 do
  x,y=flr(rnd(9))+1,flr(rnd(9))+1
  if board_t[y][x]==0 and (x~=bx or y~=by) then
   board_t[y][x]=-1
   c-=1
  end
 end
 for y=1,9 do
  for x=1,9 do
   c=0
   if board_t[y][x]==0 then
    for yy=max(1,y-1),min(9,y+1) do
     for xx=max(1,x-1),min(9,x+1) do
      if(board_t[yy][xx]<0)c-=1
     end
    end
    board_t[y][x]=-c
   end
  end
 end
 --start
 g_status=1
 last_time=time()
 key_restart=0
end

function open_block(bx,by)
 sfx(10)
 if board_t[by][bx]>-1 then
  if status_t[by][bx]==0 then
   status_t[by][bx]=5
   if(board_t[by][bx]==0)open_9_blocks(bx,by)
  end
 else
  status_t[by][bx]=6
  lose_game()
 end
end

function open_9_blocks(bx,by)
 for y=max(1,by-1),min(9,by+1) do
  for x=max(1,bx-1),min(9,bx+1) do
   if(status_t[y][x]==0)open_block(x,y)
  end
 end
end

function try_open_9()
 if bx<10 and bx>0 and by<10 and by>0 then
  c=0
  for y=max(1,by-1),min(9,by+1) do
   for x=max(1,bx-1),min(9,bx+1) do
    if(status_t[y][x]==1)c+=1
   end
  end
  if board_t[by][bx]>0 and c==board_t[by][bx] then
   open_9_blocks(bx,by)
   check_win()
  end
 end
end

function check_win()
 if(g_status~=1)return
 c=0
 for y=1,9 do
  for x=1,9 do
   if(status_t[y][x]<3)c+=1
   if(c>g_mine_count)return
  end
 end
 if c==g_mine_count then
  for y=1,9 do
   for x=1,9 do
    if(status_t[y][x]==0 or status_t[y][x]==2)status_t[y][x]=1
   end
  end
  flag_count=g_mine_count
  g_status=2
  sfx(11)
  return
 end
end

function lose_game()
 sfx(8)
 g_status=3
 for y=1,9 do
  for x=1,9 do
   if(board_t[y][x]==-1 and status_t[y][x]==0)status_t[y][x]=7
   if(board_t[y][x]~=-1 and (status_t[y][x]==1 or status_t[y][x]==2))status_t[y][x]=3
  end
 end
end

function down_9()
 if bx<10 and bx>0 and by<10 and by>0 then
  for y=max(1,by-1),min(9,by+1) do
   for x=max(1,bx-1),min(9,bx+1) do
    if(status_t[y][x]==0)status_t[y][x]=4
   end
  end
 end
end

