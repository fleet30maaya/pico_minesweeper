--input
function key_moveto(nbx,nby)
  bx,by=max(1,min(nbx,9)),max(1,min(nby,9))
  mx,my=3+bx*11+6,12+by*11+6
end

function on_input_up(b)
 if bx<10 and bx>0 and by<10 and by>0 then
  if b==0 then
   on_block_l(bx,by)
  elseif b==1 then
   on_block_r(bx,by)
  end
 end

 if mx>55 and mx<71 and my>4 and my<20 then
  if m_btn_s==4 then
   on_m_btn()
  end
 end
 m_btn_s=0
end

function on_input_down(b)
 if mx>55 and mx<71 and my>4 and my<20 then
  m_btn_s=4
 end
end

function on_input_db_up()
end

function on_input_db_down()
 try_open_9()
end

function on_duel_down()
  duel_down=true
end

function on_duel_up()
 duel_down=false
 try_open_9()
end

function on_input_hold(b)
 if(g_status~=1)return
 if m_btn_s==0 and bx<10 and bx>0 and by<10 and by>0 then
  if b==0 then
   if(status_t[by][bx]==0)status_t[by][bx]=4
  end
 end
end

function on_m_btn()
 sfx(10)
 clear_game(g_diff)
end

function on_block_l(bx,by)
 if(g_status==0)start_game(bx,by)
 if(g_status==2 or g_status==3) return
 if status_t[by][bx]==0 then
  open_block(bx,by)
  check_win()
 end
end

function on_block_r(bx,by)
 if(g_status~=1) return
 if status_t[by][bx]==2 then
  sfx(9)
  status_t[by][bx]=0
  return
 end
 if status_t[by][bx]==1 then
  sfx(9)
  status_t[by][bx]=2
  flag_count=flag_count-1
 end
 if status_t[by][bx]==0 then
  sfx(9)
  status_t[by][bx]=1
  flag_count+=1
 end
end

