--draw funcs
function d_hand(x,y)
 palt(0,false)
 palt(11,true)
 sspr(112,16,10,8,x-1,y-1)
--  spr(46,x,y)
 palt(0,true)
 palt(11,false)
end

function d_numpad(x,y)
 line(x,y,x+27,y,5)
 line(x,y+1,x,y+14,5)
 line(x+1,y+14,x+27,y+14,7)
 line(x+27,y+1,x+27,y+14,7)
 rectfill(x+1,y+1,x+26,y+13,0)
end

function d_7seg_num(n,x,y)
 n=max(0,min(n,999))
 _d_7seg(n%10,x+16,y)
 n=flr(n/10)
 _d_7seg(n%10,x+8,y)
 n=flr(n/10)
 _d_7seg(n%10,x,y)
end

seg_7={
 [0]=0b11101110,
 [1]=0b00100100,
 [2]=0b10111010,
 [3]=0b10110110,
 [4]=0b01110100,
 [5]=0b11010110,
 [6]=0b11011110,
 [7]=0b10100100,
 [8]=0b11111110,
 [9]=0b11110110,
}
seg_7_mask={
 [0]=0b10000000,
 [1]=0b01000000,
 [2]=0b00100000,
 [3]=0b00010000,
 [4]=0b00001000,
 [5]=0b00000100,
 [6]=0b00000010,
}
seg_7_offset={
 [0]={1,0,4,0},
 [1]={0,1,0,4},
 [2]={5,1,5,4},
 [3]={1,5,4,5},
 [4]={0,6,0,9},
 [5]={5,6,5,9},
 [6]={1,10,4,10},
}
function _d_7seg(n,x,y)
 for i=0,6 do
  if seg_7[n]&seg_7_mask[i]>0 then
   line(x+seg_7_offset[i][1],y+seg_7_offset[i][2],x+seg_7_offset[i][3],y+seg_7_offset[i][4],8)
  else
   line(x+seg_7_offset[i][1],y+seg_7_offset[i][2],x+seg_7_offset[i][3],y+seg_7_offset[i][4],2)
  end
 end
end

function d_block(x,y)
 d_sblock(3+x*11,12+y*11,13+x*11,22+y*11,status_t[y][x])
 d_overlap(3+x*11,12+y*11,x,y,status_t[y][x])
end

function d_sblock(x1,y1,x2,y2,s)
 if s<4 then
  line(x1,y1,x2-1,y1,7)
  line(x1,y1,x1,y2-1,7)
  line(x2,y1,x2,y2,5)
  line(x1,y2,x2-1,y2,5)
 else
  line(x1,y1,x2,y1,13)
  line(x1,y1,x1,y2,13)
  if(s==6)rectfill(x1+1,y1+1,x2,y2,8)
 end
end

num_c={
 [1]=12,
 [2]=3,
 [3]=8,
 [4]=1,
 [5]=4,
 [6]=3,
 [7]=13,
 [8]=0,
}
function d_overlap(x1,y1,x,y,s)
 if s==5 then
  if board_t[y][x]>0 and board_t[y][x]<9 then
   pal(7,num_c[board_t[y][x]],0)
   spr(board_t[y][x],x1+3,y1+2)
   pal(7,7)
  end
 elseif s==1 then
  spr(16,x1+2,y1+2)
 elseif s==2 then
  spr(17,x1+1,y1+2)
 elseif s==6 or s==7 then
  palt(0,false)
  palt(11,true)
  sspr(72,0,9,9,x1+1,y1+1)
  palt(0,true)
  palt(11,false)
 elseif s==3 then
  line(x1,y1,x1+9,y1+9,8)
  line(x1,y1+9,x1+9,y1,8)
 end
end

function d_main_button()
 d_sblock(56,5,70,19,m_btn_s)
 palt(0,false)
 palt(11,true)
 if(g_diff==1)pal(10,9,0)
 if g_status<2 then
   sspr(81,0,11,11,58,7)
 elseif g_status==2 then
   sspr(103,0,11,11,58,7)
 elseif g_status==3 then
   sspr(92,0,11,11,58,7)
 end
 if(g_diff==1)pal(10,10,0)
 palt(0,true)
 palt(11,false)
end

